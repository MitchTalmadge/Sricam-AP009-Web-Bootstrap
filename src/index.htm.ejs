<html>
<head>
    <title>IP Camera</title>
    <meta charset="UTF-8"/>
    <meta name="viewport"
          content="width=device-width, initial-scale=1"/>

    <base href="/"/>

    <!-- Favicon -->
    <link rel="apple-touch-icon"
          sizes="180x180"
          href="<%= require('./resources/favicons/apple-touch-icon.png') %>">
    <link rel="icon"
          type="image/png"
          href="<%= require('./resources/favicons/favicon-32x32.png') %>"
          sizes="32x32">
    <link rel="icon"
          type="image/png"
          href="<%= require('./resources/favicons/favicon-194x194.png') %>"
          sizes="194x194">
    <link rel="icon"
          type="image/png"
          href="<%= require('./resources/favicons/android-chrome-192x192.png') %>"
          sizes="192x192">
    <link rel="icon"
          type="image/png"
          href="<%= require('./resources/favicons/favicon-16x16.png') %>"
          sizes="16x16">
    <link rel="manifest"
          href="<%= require('!file-loader?name=./resources/json/[hash].[ext]!./resources/favicons/manifest.json') %>">
    <link rel="mask-icon"
          href="<%= require('./resources/favicons/safari-pinned-tab.svg') %>"
          color="#5bbad5">
    <meta name="theme-color"
          content="#ffffff">

    <!-- Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Droid+Sans|Roboto:300,400|Ubuntu"
          rel="stylesheet">

    <% //noinspection NodeModulesDependencies,JSUnresolvedVariable
    if(htmlWebpackPlugin.options.dev) { %>
    <!--suppress JSUnresolvedLibraryURL -->
    <script src="http://localhost:9000/webpack-dev-server.js"></script>
    <% } %>
</head>
<body>
<iframe src="app.htm" style="width: 100%; height: 100%; border: none; position: absolute;"></iframe>
</body>
</html>
